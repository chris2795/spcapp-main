<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical"
            >

    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/full_disclosure"
            android:textStyle="bold"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:textSize="25sp"
            android:background="@color/colorPrimary"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:layout_marginBottom="10dp"
            android:layout_gravity="center"/>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"

    >
        <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="5dp"
                android:layout_gravity="center"
                android:gravity="center">

            <Button android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/text_border2"
                    android:layout_marginBottom="5dp"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:id="@+id/disclosure_1"
                    android:text="@string/_4th_quarter_spp"/>
            <Button android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/text_border2"
                    android:layout_marginBottom="5dp"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:id="@+id/disclosure_2"
                    android:text="@string/_20_utilization_2018_n4th_quarter"/>
            <Button android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/text_border2"
                    android:layout_marginBottom="5dp"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:id="@+id/disclosure_3"
                    android:text="@string/app_2019"/>
            <Button android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/text_border2"
                    android:layout_marginBottom="5dp"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:id="@+id/disclosure_4"
                    android:text="@string/bid_results_2018_n4th_quarter"/>

        </LinearLayout>
        <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_gravity="center"
                tools:ignore="UselessLeaf">


        </LinearLayout>
        <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="5dp"
                android:layout_gravity="center"
                android:gravity="center">

            <Button android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/text_border2"
                    android:layout_marginBottom="5dp"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:id="@+id/disclosure_5"
                    android:text="@string/scf_4th_quarter"/>
            <Button android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/text_border2"
                    android:layout_marginBottom="5dp"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:id="@+id/disclosure_6"
                    android:text="@string/sef_utilization_n2018"/>
            <Button android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/text_border2"
                    android:layout_marginBottom="5dp"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:id="@+id/disclosure_7"
                    android:text="@string/statement_of_debt_nservices_03_24_14"/>
            <Button android:layout_width="180dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/text_border2"
                    android:layout_marginBottom="5dp"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:id="@+id/disclosure_8"
                    android:text="@string/unliquidated_n2018"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"

    >
    <LinearLayout
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="5dp">

        <Button android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/text_border2"
                android:layout_marginBottom="5dp"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:id="@+id/disclosure_9"
                android:text="@string/cdrrmf_12_31_18"/>
        <Button android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/text_border2"
                android:layout_marginBottom="5dp"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:id="@+id/disclosure_10"
                android:text="@string/manpower_ncomplement_dilg"/>

        <Button android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/text_border2"
                android:layout_marginBottom="5dp"
                android:textColor="@color/white"
                android:textSize="15sp"
                android:id="@+id/disclosure_11"
                android:text="@string/pdaf_utilization"/>
    </LinearLayout>
    </LinearLayout>
    <TextView android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="right"
              android:textStyle="bold"
              android:textAllCaps="true"
              android:textColor="@color/colorPrimary"
              android:layout_marginTop="15dp"
              android:layout_marginRight="20dp"
              android:layout_marginBottom="10dp"
              android:text="@string/cancel"
              android:id="@+id/btn_dis_cancel"
              android:textSize="15sp"
            tools:ignore="RtlHardcoded" />

</LinearLayout>